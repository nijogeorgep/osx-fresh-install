alias flushdns='sudo killall -HUP mDNSResponder'
alias git_add='git add -A'
alias myIP='ipconfig getifaddr en0'
alias localIP='ipconfig getifaddr en0'
alias convert_png='mkdir pngs; sips -s format png *.jpg --out pngs'
alias listSVNexternals='svn propget svn:externals -R .'
alias svnDiff='svn diff -r PREV:COMMITTED'
alias apacheStart='sudo apachectl start'
alias apacheStop='sudo apachectl stop'
alias apacheRestart='sudo apachectl restart'
alias mkdir='mkdir -p'
alias doSSH='ssh root@159.203.217.20'
alias errorLog='rm /tmp/php.log && tail -f /tmp/php.log'
alias latestTag='git describe --tags `git rev-list --tags --max-count=1`'
alias latestTagDiff='git show --name-only `git describe --tags` `git rev-list --tags --max-count=1`..'
alias mvim='/Applications/MacVim.app/Contents/MacOS/Vim'
alias vim='mvim -v'
# alias terminus="$HOME/pantheon-cli/bin/terminus"
alias photoshop='open -a /Applications/Adobe\ Photoshop\ CS6/Adobe\ Photoshop\ CS6.app'
#alias mysql='/Applications/MAMP/Library/bin/mysql'
alias pstorm='open -a /Applications/PhpStorm.app'
alias wstorm='open -a /Applications/WebStorm.app'
alias php_ini='sudo vim /usr/local/etc/php/7.0/php.ini'
alias php_log='tail -f ~/log/php_errors.log'
alias latestLiveTag='git describe --tags --match "pantheon_live_*" | cut -f1 -d"-"'
alias latestTestTag='git describe --tags --match "pantheon_test_*" | cut -f1 -d"-"'
alias branchUpdates='for k in `git branch | perl -pe s/^..//`; do echo -e `git show --pretty=format:"%Cgreen%ci %Cblue%cr%Creset" $k -- | head -n 1`\\t$k; done | sort -r'
